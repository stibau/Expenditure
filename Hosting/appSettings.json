{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console"        
      }
    ] 
  },
"Kestrel": {
  "Endpoints": {
     "Http": {
       "Url": "http://0.0.0.0:5050"
       // "Protocols:"http1" -> Not necessary because http2 needs ssl. 
       // Since we configure for plain http, Kestrel will select HTTP/1.1 automatically
     }
    }
  },
  "Swagger": {
    "Enabled": false
  }
}